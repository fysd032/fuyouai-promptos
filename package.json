{
  "name": "fuyouai-promptos",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",

    "gen:mmap": "tsx scripts/gen-frontend-module-id-map.ts",
    "check:mmap": "npm run gen:mmap && git diff --exit-code",
    "gen:prompt-bank": "tsx scripts/generate-prompt-bank.ts",

    "gen:prompts": "node scripts/generate-prompt-registry.js",
    "verify:core": "node -e \"const fs=require('fs');const p='lib/promptos/prompts.generated.ts';const s=fs.existsSync(p)?fs.readFileSync(p,'utf8'):''; if(!s.includes('core.task_breakdown_engine.basic')){console.error('Missing core prompt key in generated registry'); process.exit(1)}\"",

    "prebuild": "npm run gen:prompt-bank && npm run gen:mmap && npm run gen:prompts && npm run verify:core"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "next": "16.0.7",
    "openai": "^6.10.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
